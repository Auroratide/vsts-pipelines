{
  "name": "vsts-pipelines",
  "version": "0.0.1",
  "description": "A piplelines view of VSTS Build and Release",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all build start-app",
    "start:watch": "npm-run-all build start-app:watch",
    "start-app": "node dist/server.js",
    "start-app:watch": "npm-run-all --parallel build:watch start-app",
    "build": "mkdir -p dist/views && cp -a src/server/views/. dist/views && webpack",
    "build:watch": "webpack --watch",
    "clean": "rm -rf dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vsts",
    "pipelines",
    "build",
    "monitor"
  ],
  "author": "Timothy Foster",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.8.1"
  }
}
