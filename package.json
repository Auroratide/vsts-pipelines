{
  "name": "vsts-pipelines",
  "version": "0.0.1",
  "description": "A piplelines view of VSTS Build and Release",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all build start-app",
    "start:watch": "npm-run-all build start-app:watch",
    "start-app": "node dist/server.js",
    "start-app:watch": "npm-run-all --parallel build:watch start-app",
    "build": "mkdir -p dist/views && cp -a src/server/views/. dist/views && webpack",
    "build:watch": "webpack --watch",
    "clean": "rm -rf dist/",
    "test": "npm-run-all lint test:all",
    "test:all": "npm-run-all test:unit",
    "test:unit": "NODE_ENV=test mocha --require babel-core/register --require ignore-styles --require ./test/enzyme-setup.js './src/**/*.spec.{js,jsx}'",
    "lint": "eslint 'src/**/*.{js,jsx}'"
  },
  "keywords": [
    "vsts",
    "pipelines",
    "build",
    "monitor"
  ],
  "author": "Timothy Foster",
  "repository": {
    "type": "git",
    "url": "https://github.com/Auroratide/vsts-pipelines.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "testdouble": "^3.5.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
